# 开发环境配置
NODE_ENV=development

# 端口配置
FRONTEND_PORT=3000
BACKEND_PORT=3001

# 前后端URL
FRONTEND_URL=http://localhost:3000
BACKEND_URL=http://localhost:3001

# 数据库配置
DATABASE_URL=file:./data/sqlite-dev.db

# JWT密钥（开发环境）
JWT_SECRET=dev_secret_key_change_in_production
JWT_EXPIRES_IN=24h

# 日志配置
LOG_LEVEL=debug

# Redis配置（开发环境可选）
REDIS_URL=redis://localhost:6380

# 开发工具配置
ENABLE_SWAGGER=true
ENABLE_CORS=true