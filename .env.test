# 测试环境配置
NODE_ENV=test

# 端口配置
FRONTEND_PORT=3002
BACKEND_PORT=3003

# 前后端URL
FRONTEND_URL=http://localhost:3002
BACKEND_URL=http://localhost:3003

# 数据库配置（内存数据库）
DATABASE_URL=:memory:

# JWT密钥（测试环境）
JWT_SECRET=test_secret_key
JWT_EXPIRES_IN=1h

# 日志配置
LOG_LEVEL=error

# 测试配置
ENABLE_SWAGGER=false
ENABLE_CORS=true
TEST_TIMEOUT=30000